<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/customButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/isemsari_custom_button"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:backgroundTint="@color/white"
            android:text="LOG IN "
            android:textColor="@color/brownish"
            android:textSize="12dp"
            android:textStyle="bold"
            app:cornerRadius="30dp"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/drright"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8.2dp"
            android:translationZ="10dp"
            app:layout_constraintBottom_toBottomOf="@id/isemsari_custom_button"
            app:layout_constraintEnd_toEndOf="@id/isemsari_custom_button"
            app:layout_constraintTop_toTopOf="@id/isemsari_custom_button">

        </ImageView>

        <ImageView
            android:id="@+id/drLeft"
            android:layout_width="22.6dp"
            android:layout_height="23dp"
            android:layout_marginLeft="8.2dp"
            android:translationZ="10dp"
            app:layout_constraintBottom_toBottomOf="@id/isemsari_custom_button"
            app:layout_constraintStart_toStartOf="@id/isemsari_custom_button"
            app:layout_constraintTop_toTopOf="@id/isemsari_custom_button">

        </ImageView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</merge>